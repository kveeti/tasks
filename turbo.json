{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["dist/**", ".next/**", "!.next/cache/**", "public/dist/**"],
			"dependsOn": ["^build"],
			"env": ["NEXT_PUBLIC_API_HOST"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dbpush": {
			"cache": false,
			"persistent": true
		}
	}
}
