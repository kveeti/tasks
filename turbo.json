{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		},
        "buildprod": {
			"outputs": ["dist/**"],
			"dependsOn": ["^buildprod"]
		},
		"dev": {
            "dependsOn": ["build"],
			"cache": false,
			"persistent": true
		},
		"dbpush": {
			"cache": false,
			"persistent": true
		},
		"preview": {
			"outputs": ["dist/**"],
			"dependsOn": ["build"]
		}
	}
}
