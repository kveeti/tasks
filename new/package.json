{
	"name": "tasks-monorepo",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"dev": "APP_ENV=dev op run --env-file=\"./.1password.env\" -- turbo dev",
		"clean": "rm -rf ./node_modules ./.turbo ./**/**/node_modules ./**/**/dist ./**/**/.turbo",
		"db": "docker run --rm -it -p 3306:3306 -e MYSQL_ROOT_PASSWORD=secret -e MYSQL_DATABASE=tasks -e MYSQL_USER=tasks -e MYSQL_PASSWORD=tasks mysql"
	},
	"devDependencies": {
		"turbo": "^1.9.3",
		"typescript": "^5.0.2"
	},
	"packageManager": "yarn@1.22.19",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	}
}
