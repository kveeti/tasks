  "db_name": "PostgreSQL",
  "query": "\n                SELECT tasks.*, tags.label AS tag_label, tags.color AS tag_color\n                FROM tasks\n                INNER JOIN tags ON tasks.tag_id = tags.id\n                WHERE tasks.user_id = $1\n                ORDER BY tasks.id DESC\n                LIMIT $2;\n            ",
